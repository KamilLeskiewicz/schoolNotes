{
  "version": 2,
  "builds": [
    {
      "src": "web/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/data", "dest": "api/index.js" },
    { "src": "/(.*)", "dest": "web/src/index.js" }
  ]
}
